# Example Environment Variables
# Copy to .env and fill in your values

# ZendFi API Key (required)
# Get yours at https://zendfi.tech
ZENDFI_API_KEY=zk_test_your_key_here

# ZendFi Mode: 'test' for devnet, 'live' for mainnet
ZENDFI_MODE=test

# User's main Solana wallet (for session key creation)
ZENDFI_USER_WALLET=your_solana_wallet_address

# OpenAI API Key (for GPT-4 examples)
OPENAI_API_KEY=sk-your_openai_key_here

# Optional: Anthropic API Key (for Claude examples)
# ANTHROPIC_API_KEY=sk-ant-your_key_here
